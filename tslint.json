{
    "extends": "tslint:latest",
    "rules": {
        "object-literal-sort-keys": [
            true,
            "match-declaration-order"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "functions": "never"
                }
            }
        ],
        "interface-name": [
            true,
            "never-prefix"
        ]
    }
}